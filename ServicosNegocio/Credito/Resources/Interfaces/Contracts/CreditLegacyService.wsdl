			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="CreditLegacyService">
		<wsdl:port binding="tn:CreditLegacyServiceSOAP" name="CreditLegacyServiceSOAP">
			<soap:address location="http://cursosoa.org/" />
		</wsdl:port>
