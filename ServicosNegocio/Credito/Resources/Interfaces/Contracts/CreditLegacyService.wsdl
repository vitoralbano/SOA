	<wsdl:message name="CreditLegacyResponse">
		<wsdl:part element="ser:ResultadoCreditLegacy" name="ResultadoCreditLegacy" />
	</wsdl:message>

	<wsdl:portType name="CreditLegacyService">
		<wsdl:operation name="ConsultarCreditLegacy">
